@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix org: <http://www.w3.org/ns/org#> .
@prefix schema: <https://schema.org/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix eli: <http://data.europa.eu/eli/ontology#> .
@prefix mdrnal: <http://publications.europa.eu/resource/authority/language/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix void: <http://rdfs.org/ns/void#> .
@prefix blnal: <https://berlin.github.io/lod-vocabulary/blnal/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix swformats: <https://www.w3.org/ns/formats/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Vocabulary Terms

blnal: a owl:Ontology ;
    rdfs:label "'Berliner Allerlei'-Vokabular"@de ;
    rdfs:label "'Berliner Allerlei' Vocabulary (Berlin Assortment)"@en ;
    rdfs:comment """Ein Vokabular unzusammenhängender Klassen, Properties und anderer Terminologie für Linked Open Data der Berliner Verwaltung. Die Terme haben weder inhaltlich in ein anderes Vokabular gepasst, noch ein jeweils eigenes Vokabular gerechtfertigt."""@de ;
    rdfs:comment """A vocabulary for disconnected classes, properties and other kinds of terminology for linked open data from the public administration of Berlin. Each of the terms did neither fit thematically into another existing vocabulary, nor did the warrant a new vocabular on their own."""@en ;
    void:dataDump <https://raw.githubusercontent.com/berlin/lod-vocabulary/main/data/blnal/blnal.ttl> ;
    void:feature swformats:Turtle ;
    dcterms:created "2025-11-27"^^xsd:date ;
    dcterms:modified "2026-01-13"^^xsd:date ;
    dcterms:license <https://creativecommons.org/publicdomain/zero/1.0/> ;
    owl:versionInfo "0.0.3" ;
    blnal:changelog <https://github.com/berlin/lod-vocabulary/blob/main/data/blnal/CHANGELOG.md> ;
    rdfs:seeAlso <https://raw.githubusercontent.com/berlin/lod-vocabulary/refs/heads/main/data/blnal/stats.ttl> ;
.

blnal:acronym a owl:AnnotationProperty ;
    rdfs:label "Acronym"@en ;
    rdfs:label "Acronym"@de ;
    rdfs:comment "Ein Acronym als alternativer Bezeichner für beliebige Entitäten."@de ;
    rdfs:comment "An acronym as an alternative label for any kind of entity."@en ;
    rdfs:subPropertyOf skos:altLabel ;
    rdfs:isDefinedBy blnal: ;
    rdfs:seeAlso <https://github.com/berlinonline/lod-berlin-lor/blob/with_vocab/data/vocab/units.ttl#L80> ;
.

blnal:includesPersonalData a owl:DatatypeProperty ;
    rdfs:label "personenbezogene Daten"@de ;
    rdfs:label "personal data"@en ;
    rdfs:comment "Zeigt an, dass die Primärdaten eines Datensatzes personenbezogene Daten enthalten."@de ;
    rdfs:comment "Indicates that the primary data of a dataset contains personal data."@en ;
    rdfs:domain dcat:Dataset ;
    rdfs:range xsd:boolean ;
    rdfs:isDefinedBy blnal: ;
.

blnal:personalDataExemption a owl:DatatypeProperty ;
    rdfs:label "Bereitstellung personenbezogener Daten aufgrund eines Gesetzes"@de ;
    rdfs:label "Publication of personal data because of specific laws"@en ;
    rdfs:comment "Daten mit Personenbezug dürfen standardmäßig nicht als offene Daten veröffentlicht werden. Es gibt allerdings in bestimmten Fällen Gesetze oder andere Regelungen, die die Veröffentlichung trotzdem erlauben bzw. erfordern. Diese Property zeigt an, dass dies für einen Datensatz zutrifft-"@de ;
    rdfs:comment "By default personal data must not be published as open data. However, in some cases there are laws that allow or demand the publication of personal data. This property indicates that this is the case for a dataset."@en ;
    rdfs:domain dcat:Dataset ;
    rdfs:range xsd:boolean ;
    rdfs:isDefinedBy blnal: ;
.

blnal:dataAnonmized a owl:DatatypeProperty ;
    rdfs:label "Daten anonymisiert"@de ;
    rdfs:label "Data anonymised"@en ;
    rdfs:comment "Zeigt an, dass die Primärdaten (oder die Distribution) eines Datensatzes anonymisiert wurden. Es handelt sich dabei lediglich um die allgemeine Information, dass Schritte zur Anonymisierung unternommen wurden. Es gibt keine Angabe über den Grad der Anonymisierung (oder Pseudoanonymisierung)."@de ;
    rdfs:comment "Indicates that the primary data (or distribution) of a dataset has been anonymized in some way. This is just a generic information that some effort has been made towards anonymization – mo guarantees are made about the level of anonymization (or pseudo-anonymization)."@en ;
    rdfs:domain dcat:Dataset ;
    rdfs:range xsd:boolean ;
    rdfs:isDefinedBy blnal: ;
.

blnal:changelog a owl:ObjectProperty;
    rdfs:label "Changelog"@de ;
    rdfs:label "Changelog"@en ;
    rdfs:comment "Link zum Changelog eines Projekts, etwas eines Datensatzes oder einer Ontologie."@de ;
    rdfs:comment "Link to the changelog of a project, such as a dataset or an ontology."@en ;
    rdfs:subPropertyOf foaf:page ;
    rdfs:isDefinedBy blnal: ;
.

# EGovG Bln als eli:LegalResource

blnal:egovg a eli:LegalResource ;
    rdfs:label "E-Government-Gesetz Berlin"@de ;
    rdfs:comment """Das „Gesetz zur Förderung des E-Government“ (E-Government-Gesetz Berlin - EGovG Bln) vom 30. Mai 2016."""@de ;
    rdfs:isDefinedBy blnal: ;
    eli:title "E-Government-Gesetz Berlin"@de ;
    eli:title_short "EGovG Bln"@de ;
    eli:title_alternative "Gesetz zur Förderung des E-Government"@de ;
    eli:language mdrnal:DEU ;
    eli:published_in_format <https://gesetze.berlin.de/perma?d=jlr-EGovGBErahmen> ;
    eli:format <https://www.iana.org/assignments/media-types/text/html> ;
.
